    "core-js": "^2.6.1",
    "lodash": "^4.17.11",
    "ngx-prism-highlight": "^1.0.1",
    "prismjs": "^1.15.0",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.1.1",
    "@angular-builders/dev-server": "^7.1.1",
