    "husky": "^1.3.0",
    "lint-staged": "^8.1.0",
    "ng-packagr": "^4.2.0",
    "prettier": "^1.15.3",
    "ts-config-mobiquity-angular": "^5.0.0",
    "tsickle": ">=0.29.0",
